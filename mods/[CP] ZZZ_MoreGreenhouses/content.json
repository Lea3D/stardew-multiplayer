// Compatability Greenhouse
      {
        "Action": "Load",
        "Target": "Maps/Greenhouse2",
        "FromFile": "assets/Greenhouse.tmx"
      },
      {
        "Action": "EditData",
        "When":{"Compatibility Mode": "true", "Buildable Greenhouse": "True"},
        "Target": "Data/Buildings",
        "TargetField": ["NewGreenhouse"],
        "Entries": { "IndoorMap": "Greenhouse2" }
      },
      {
        "Action": "EditData",
        "When": {"Compatibility Mode": "true", "Buildable Mini Greenhouse": "True"},
        "Target": "Data/Buildings",
        "TargetField": ["MiniNewGreenhouse"],
        "Entries": { "IndoorMap": "Greenhouse2" }
      },
  
