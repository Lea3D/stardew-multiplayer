{
	"Format": "2.0.0",

"ConfigSchema": { 
// Main Options

          "Main Greenhouse Type": {
                     "AllowValues": "Mini, Normal",
                     "Default": "Normal",
                     "Section": "Options",
                     "Description": "Change Main Greenhouse Size"
                             },
          "Buildable Greenhouse": {
                     "AllowValues": "True, False",
                     "Default": "True",
                     "Section": "Options",
                     "Description": "Enable Normal Greenhouse"
                             },
          "Buildable Mini Greenhouse": {
                      "AllowValues": "True, False",
                      "Default": "True",
                      "Section": "Options",
                      "Description": "Enable Mini Greenhouse"
                              },

//Fixes Mod Compatibility issues
          "Compatibility Mode":{
                     "AllowValues":  "true, false",
                     "Default": "false", 
                     "Section": "Enable for Mod Compatability issues",
                     "Description": "This toggle bypasses any mod inputs and creates an independant building using its own assets"
                              },
//Config Menu
         "Build Cost":{
                     "AllowValues":  "150000, 50000, 0",
                     "Default":  "150000",
                     "Section": "Costs Changes",
                     "Description": "Main Price"
                      },
         "Materials":{
                     "AllowValues":  "Normal, Cheap, Free",
                     "Default": "Normal",
                     "Section": "Costs Changes",
                     "Description": "Resorces Cost"
                     },
         "Build Time":{
                     "AllowValues":  "3 Days, 2 Days, 1 Day, Instant",
                     "Default": "3 Days",
                     "Section": "Costs Changes",
                     "Description": "Time Requirements"
                     },



//Toggle off for instant access to greenhouse if you havent unlocked at pantry yet
         "Build Requirements":{
                     "AllowValues":  "true, false",
                     "Default": "true", 
                     "Section": "Greenhouse Requirements Unlock",
                     "Description": "Toggle off for instant access to greenhouse if you havent unlocked at pantry yet"
                              }
                  },		

"Changes":[
//Greenhouse Fix
      {
       "Action": "EditMap",
       "Target": "Maps/Greenhouse",
       "MapProperties": { "IsGreenhouse": "T"}
      },

      
//Main Greenhouse
